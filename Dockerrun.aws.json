{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "api",
            "image": "projectdiaspora/api-feathers",
            "hostname": "api",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "projectdiaspora/api-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["api"],
            "memory": 128
        }
    ]
}
